#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules/@netlify/zip-it-and-ship-it/node_modules:/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules/@netlify/node_modules:/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules:/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules/@netlify/zip-it-and-ship-it/node_modules:/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules/@netlify/node_modules:/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules:/mnt/h/Projects/maestroLanding/landing-maestro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules/@netlify/zip-it-and-ship-it/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../@netlify+zip-it-and-ship-it@12.1.4_rollup@4.43.0/node_modules/@netlify/zip-it-and-ship-it/bin.js" "$@"
fi
